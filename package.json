{
  "name": "online_tally",
  "version": "1.0.0",
  "description": "sharedcounter",
  "main": "server.js",
  "scripts": {
    "client": "cd client && npm run start --prefix client",
    "server": "nodemon server.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "heroku-postbuild": "npm install && cd client && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/thiagogpa/react-shared-counter"
  },
  "author": "Thiago",
  "license": "ISC",
  "homepage": "https://sharedcounter.herokuapp.com/",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.6.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mongoose": "^5.9.22",
    "uuid": "^8.2.0",
    "socket.io": "^2.4.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
